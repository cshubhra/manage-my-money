<%= link_to 'Wszystkie raporty', reports_url, :id => 'reports-index' %>
<% unless @virtual %>
| <%= link_to "Edytuj", edit_report_url(@report), obj_hash_id('report-edit', @report) %>
  <% unless @report.temporary %>
  | <%= link_to "Usun", report_path(@report), {:method => :delete}.merge(obj_hash_id('report-delete', @report)) %>
  <% end %>
<% end %>
